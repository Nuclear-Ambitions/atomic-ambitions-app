    "@types/react-dom": "^19.2.2",
    "cypress": "^15.4.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "kysely-codegen": "^0.19.0",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.14",
